<?xml version="1.0" encoding="UTF-8"?>
<om:source xmlns:om="http://brisskit.org/xml/onyxmetadata/v1.0/om">
   <om:name>onyx</om:name>
   <om:stage>
      <om:name>RiskFactorQuestionnaire</om:name>
      <om:variable>
         <om:name>QuestionnaireRun</om:name>
         <om:variable>
            <om:name>version</om:name>
            <om:type>text</om:type>
         </om:variable>
         <om:variable>
            <om:name>locale</om:name>
            <om:type>locale</om:type>
         </om:variable>
         <om:variable>
            <om:name>timeStart</om:name>
            <om:type>date</om:type>
         </om:variable>
         <om:variable>
            <om:name>timeEnd</om:name>
            <om:type>date</om:type>
         </om:variable>
         <om:variable>
            <om:name>user</om:name>
            <om:type>text</om:type>
         </om:variable>
      </om:variable>
      <om:variable>
         <om:name>QuestionnaireMetric</om:name>
         <om:variable>
            <om:name>section</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>page</om:name>
            <om:type>text</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>duration</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>questionCount</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
         <om:variable>
            <om:name>missingCount</om:name>
            <om:type>integer</om:type>
            <om:repeatable>true</om:repeatable>
         </om:variable>
      </om:variable>
      <om:section>
         <om:name>ALCOHOL</om:name>
         <om:question>
            <om:name>alcohol_ever</om:name>
            <om:label>Have you ever drunk alcohol?</om:label>
            <om:variable>
               <om:name>alcohol_ever</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_regular</om:name>
            <om:label>Have you ever drunk alcohol at least once per week?</om:label>
            <om:variable>
               <om:name>alcohol_regular</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_now</om:name>
            <om:label>Do you currently drink alcohol at least once per week?</om:label>
            <om:variable>
               <om:name>alcohol_now</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>N</om:name>
                  <om:label>No</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Y</om:name>
                  <om:label>Yes</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_beer_cat</om:name>
            <om:label>How many pints of beer do you drink in a typical week?</om:label>
            <om:variable>
               <om:name>alcohol_beer_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>OPEN_N</om:name>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_beer</om:name>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_wwine_cat</om:name>
            <om:label>How many glasses of white wine do you drink in a typical week?</om:label>
            <om:variable>
               <om:name>alcohol_wwine_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>OPEN_N</om:name>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_wwine</om:name>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_rwine_cat</om:name>
            <om:label>How many glasses of red wine do you drink in a typical week?</om:label>
            <om:variable>
               <om:name>alcohol_rwine_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>OPEN_N</om:name>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_rwine</om:name>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_rowine_cat</om:name>
            <om:label>How many glasses of rose wine do you drink in a typical week?</om:label>
            <om:variable>
               <om:name>alcohol_rowine_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>OPEN_N</om:name>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_rowine</om:name>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_spirits_cat</om:name>
            <om:label>How many measures of spirits do you drink in a typical week?</om:label>
            <om:variable>
               <om:name>alcohol_spirits_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>OPEN_N</om:name>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_spirits</om:name>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_onset_cat</om:name>
            <om:label>How old were you when you began drinking alcohol?</om:label>
            <om:variable>
               <om:name>alcohol_onset_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>AGE</om:name>
                  <om:label>AGE</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_onset</om:name>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_stop_cat</om:name>
            <om:label>How old were you when you stopped drinking alcohol?</om:label>
            <om:variable>
               <om:name>alcohol_stop_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>AGE7</om:name>
                  <om:label>AGE</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_stop</om:name>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>alcohol_last_cat</om:name>
            <om:label>When was the last time you had a drink containing alcohol?</om:label>
            <om:variable>
               <om:name>alcohol_last_cat</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>TIME_24</om:name>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>MORE_24_HOURS</om:name>
                  <om:label>More than 24 hours.</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_last_day</om:name>
                  <om:label>Day</om:label>
                  <om:type>text</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_last_hour</om:name>
                  <om:label>Time: HH</om:label>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>alcohol_last_min</om:name>
                  <om:label>MM</om:label>
                  <om:type>integer</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
      </om:section>
      <om:section>
         <om:name>Diet_Exercise</om:name>
         <om:question>
            <om:name>risk_diet</om:name>
            <om:label>How would you describe your diet?</om:label>
            <om:variable>
               <om:name>risk_diet</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>Vegetarian</om:name>
                  <om:label>Vegetarian</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Vegan</om:name>
                  <om:label>Vegan</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Pescatarian</om:name>
                  <om:label>Pescatarian</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Omnivore</om:name>
                  <om:label>Omnivore</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>risk_workactivity</om:name>
            <om:label>If you work, does your work involve any physical activity?</om:label>
            <om:variable>
               <om:name>risk_workactivity</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>No_work</om:name>
                  <om:label>I don't currently work</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>No_physical</om:name>
                  <om:label>My work involves no physical activity</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Light_physical</om:name>
                  <om:label>My work involves light physical activity</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Heavy_physical</om:name>
                  <om:label>My work involves heavy physical activity</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
         <om:question>
            <om:name>risk_exercise</om:name>
            <om:label>Apart from work, do you undertake any regular physical exercise over and above that of daily living?</om:label>
            <om:variable>
               <om:name>risk_exercise</om:name>
               <om:type>text</om:type>
               <om:variable>
                  <om:name>NONE</om:name>
                  <om:label>None</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Light_activity</om:name>
                  <om:label>Light activity (walking, golf, etc.)</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Moderately_vigorous</om:name>
                  <om:label>Moderately vigorous activity (gym, sports, etc.) for 20 minutes or more, once or twice per week</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Heavy_vigorous</om:name>
                  <om:label>Heavy vigorous activity more than twice per week</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>Extreme_exercise</om:name>
                  <om:label>Extreme exercise (long distance cycling, marathon running, etc)</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>PNA</om:name>
                  <om:label>Prefer not to answer</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>DK</om:name>
                  <om:label>Don't know</om:label>
                  <om:type>boolean</om:type>
               </om:variable>
               <om:variable>
                  <om:name>comment</om:name>
                  <om:type>text</om:type>
               </om:variable>
            </om:variable>
         </om:question>
      </om:section>
   </om:stage>
</om:source>